.container
	%h1 All Vendors
	%hr
	= ('Music')
	%hr
	- if @vendors.any?
		- @vendors.each do |vendor|
			= link_to vendor.email, vendor_path(vendor)
			%br
		%button.florists{data: { type: "Florist"}}
			Florists
		%button.attire{data: { type: "Attire"}}
			Attire
		%h3.all-florists
		%h3.all-attire
	- else
		%h3 There are currently no vendors.
